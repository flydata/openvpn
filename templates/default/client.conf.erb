client
dev tun
proto <%= node['openvpn']['proto'] %>
remote <%= node['openvpn']['gateway'] %> <%= node['openvpn']['port'] %>
resolv-retry infinite
keepalive 10 120
auth-user-pass
auth-nocache
nobind
persist-key
persist-tun
ns-cert-type server
ca ca.crt
cert <%= @username %>.crt
key <%= @username %>.key
comp-lzo
verb 3
